          </svg>
          Upload
        </button>

        <span class="sv-header">Saved Notes</span>
        <!-- COMPONENT: Saved Notes Container at left panel -->
        <%- include('saved-notes.ejs', { notes: notes , savedNotes: savedNotes}) %>


        <button class="right-panel-btn" onclick="location.href='/support'">Help & Support</button>
        <button class="right-panel-btn" onclick="window.open('https://app.hubspot.com/guide-creator/g/YPlbIIXPTt?uuid=f456224c-8bcd-4673-8a7d-573176e779bb', '_blank'); return false">User Guide</button>
        <button class="right-panel-btn" onclick="location.href='/about-us'">About</button>
      </div>
<% } %>